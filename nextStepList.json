  "/send-register-data": {
       "handler":"async ({specialCall})=>{/*SERVER PART:*/const [userInput,userID,opCode]=specialCall; console.log('Passed To Handler /send-register-data: ',userInput,userID,opCode); const opCodesCallee={0:'/customer-register-phone',1:'/customer-register-location',2:'/customer-register-pin'}; const _serverResponse=(await Axios.post('http://127.0.0.1:3000/register-customer-data/',{userID,opCode,userInput})).data; /*  SERVER ERROR HANDLING: */ if(_serverResponse.error){console.log('SERVER ERROR: ', _serverResponse.error); return {call:{call: opCodesCallee[opCode], prompt:{prompt:'Error Trying to Save Your Data, Try again:'} }}; }  /*+++++++++++*/ if(opCode=='confirm'){return {call:{ call:'/customer-regiser-confirm', _requiredParameters:['USER_ID'] }}  }else{ if(!opCodesCallee[opCode+1]){return {call:{call:'/customer-register-confirm'}}};  return {call:{call:opCodesCallee[opCode+1]}} };  }"    
  },
